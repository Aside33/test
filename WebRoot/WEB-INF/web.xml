<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>register</servlet-name>
    <servlet-class>myservlet.control.register</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>myservlet.control.login</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>upload</servlet-name>
    <servlet-class>myservlet.control.upload</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>lookRecord</servlet-name>
    <servlet-class>myservlet.control.lookRecord</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>modifyPassword</servlet-name>
    <servlet-class>myservlet.control.modifyPassword</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getOldMess</servlet-name>
    <servlet-class>myservlet.control.getOldMess</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>modifyOldMess</servlet-name>
    <servlet-class>myservlet.control.modifyOldMess</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>exit</servlet-name>
    <servlet-class>myservlet.control.exit</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookRecord</servlet-name>
    <url-pattern>/lookRecord</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>modifyPassword</servlet-name>
    <url-pattern>/modifyPassword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getOldMess</servlet-name>
    <url-pattern>/getOldMess</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>modifyOldMess</servlet-name>
    <url-pattern>/modifyOldMess</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>exit</servlet-name>
    <url-pattern>/exit</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
